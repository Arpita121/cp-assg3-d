#include<stdio.h>
int main(){
    int i;
    char a[27];
    char *ptr;

    printf("Print all the English Alphabets:\n");

    ptr=a;

    for(i=0;i<26;i++){
        *ptr=i + 'A';
        ptr++;
    }
    ptr=a;
    printf("The alphabets are.....\n");
    for(i=0;i<26;i++){
        printf("%c",*ptr);
        ptr++;
    }
    printf("\n\n");
return 0;
}
